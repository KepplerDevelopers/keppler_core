- objects.each do |<%= singular_table_name %>|
  %tr{ id: "row-#{<%= singular_table_name %>.id}" }
    %td.item-check{ style: 'width: 10%' }
      - if can?(model).destroy_multiple?
        .checkbox
          %label{ for: "checkbox-#{<%= singular_table_name %>.id}" }
            %input{ type: 'checkbox', id: "checkbox-#{<%= singular_table_name %>.id}", 'checklist-model': 'check', 'checklist-value': "#{<%= singular_table_name %>.id}", '@click': "listDelete(#{<%= singular_table_name %>.id})"}
            %span.cr
              %i.cr-icon.glyphicon.glyphicon-ok
  <%- attributes.each_with_index do |attribute, index| -%>
    <%- if index < 6 && (%i[string integer datetime].include?(attribute.type) && %w[position].include?(attribute.name)) -%>
      <%- if SINGULAR_ATTACHMENTS.include?(attribute.name) -%>
    %td
      = link_to admin_<%= namespaced_path.split('_').drop(1).join('_') %>_<%= singular_table_name %>_path(<%= singular_table_name %>, search: @query), class: 'show-row' do
        %span
          = image_tag <%= singular_table_name %>.<%= attribute.name %>.url unless <%= singular_table_name %>.<%= attribute.name %>.url.blank?
      <%- elsif PLURAL_ATTACHMENTS.include?(attribute.name) -%>
    %td
      = link_to admin_<%= namespaced_path.split('_').drop(1).join('_') %>_<%= singular_table_name %>_path(<%= singular_table_name %>, search: @query), class: 'show-row' do
        %span
          - <%= singular_table_name %>.<%= attribute.name %>.each do |attach|
            = image_tag attach.url unless attach.url.blank?
      <%- elsif attribute.reference? -%>
    %td
      = link_to admin_<%= namespaced_path.split('_').drop(1).join('_') %>_<%= singular_table_name %>_path(<%= singular_table_name %>, search: @query), class: 'show-row' do
        %span
          <%- %w[title name first_name full_name].each do |title| -%>
          <%- if attribute.name.classify.constantize.column_names.include?(title) -%>
          = <%= singular_table_name %>.<%= attribute.name %>.<%= title -%>
          <%- end -%>
          <%- end -%>
      <%- elsif attribute.type.eql?(datetime) -%>
    %td
      = link_to admin_<%= namespaced_path.split('_').drop(1).join('_') %>_<%= singular_table_name %>_path(<%= singular_table_name %>, search: @query), class: 'show-row' do
        %span
          = distance_of_time_in_words_to_now(<%= singular_table_name %>.<%= attribute.name %>)
      <%- else -%>
    %td
      = link_to admin_<%= namespaced_path.split('_').drop(1).join('_') %>_<%= singular_table_name %>_path(<%= singular_table_name %>, search: @query), class: 'show-row' do
        %span
          = <%= singular_table_name %>.<%= attribute.name %>
      <%- end -%>
    <%- end -%>
  <%- end -%>
    %td.dropdown{ style: 'width: 15px;padding-right:15px;' }
      %a.action-btn.pull-right{ id: "menu-#{<%= singular_table_name %>.id}", 'aria-expanded': 'false', 'aria-haspopup': 'true', 'data-toggle': 'dropdown'}
        %i.icon-options-vertical
      %ul.actions.dropdown-menu{"aria-labelledby" => "menu-#{<%= singular_table_name %>.id}" }
        - if can?(model).index?
          %li
            = link_to admin_<%= namespaced_path.split('_').drop(1).join('_') %>_<%= singular_table_name %>_path(<%= singular_table_name %>), title: t('keppler.actions.show') do
              %i.icon-eye
              = t('keppler.actions.show')
        - if can?(model).update?
          %li
            = link_to edit_admin_<%= namespaced_path.split('_').drop(1).join('_') %>_<%= singular_table_name %>_path(<%= singular_table_name %>), title: t('keppler.actions.edit') do
              %i.icon-pencil
              = t('keppler.actions.edit')
        - if can?(model).clone?
          %li
            = link_to admin_<%= singular_table_name %>_clone_path(<%= singular_table_name %>), title: t('keppler.actions.clone') do
              %i.icon-docs
              = t('keppler.actions.clone')
        - if can?(model).destroy?
          %li
            = link_to admin_<%= namespaced_path.split('_').drop(1).join('_') %>_<%= singular_table_name %>_path(<%= singular_table_name %>), title: t('keppler.actions.delete'), method: :delete, data: { confirm: t('keppler.messages.data_confirm') } do
              %i.icon-trash
              = t('keppler.actions.delete')

    %td{ style: 'width: 5%;' }
      %a.drop{ href: '#' }
        %i.icon-cursor-move
