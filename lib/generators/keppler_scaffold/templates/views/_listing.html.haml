- objects.each do |<%= singular_table_name %>|
  %tr{ id: "row-#{<%= singular_table_name %>.id}" }
    - if can? :destroy_multiple, <%= singular_table_name.camelcase %>
      %td.item-check{style:"width: 10%"}
        = link_to "", admin_<%= singular_table_name %>_path(<%= singular_table_name %>), remote: true, class: "show-row"
        .pretty.p-default.p-curve.p-thick.p-smooth
          %input{ type:"checkbox", class: "filled-in", id: "checkbox-#{<%= singular_table_name %>.id}", 'checklist-model'=>"check", 'checklist-value'=>"#{<%= singular_table_name %>.id}", "@click" => "listDelete(#{<%= singular_table_name %>.id})"}
          .state.p-danger-o
            %label{ for: "checkbox-#{<%= singular_table_name %>.id}" }
    <%- attributes.each do |attribute| -%>
	 		<%- if @attachments.include?(attribute.name) -%>
	 	%td.center
	 		= link_to '', admin_<%= singular_table_name %>_path(<%= singular_table_name %>, search: @query), remote: true, class: 'show-row'
	 		%span
	 			= image_tag <%= singular_table_name %>.<%= attribute.name %>
	 		<%- end -%>
	 	<%- end -%>
		<%- attributes.each_with_index do |attribute, index| -%>
	 		<%- if index < 6 -%>
	 			<%- unless attribute.type.to_s == 'text' -%>
	 				<%- unless @attachments.include?(attribute.name) -%>
	 					<%- if index < 2 -%>
	 	%td.center
	 		= link_to '', admin_<%= singular_table_name %>_path(<%= singular_table_name %>, search: @query), remote: true, class: 'show-row'
	 		%span
	 			= <%= singular_table_name %>.<%= attribute.name %>
	 					<%- else -%>
	 	%td.center.visible-lg.visible-md
	 		= link_to '', admin_<%= singular_table_name %>_path(<%= singular_table_name %>, search: @query), remote: true, class: 'show-row'
	 		%span
	 			= <%= singular_table_name %>.<%= attribute.name %>
	 					<%- end -%>
	 				<%- end -%>
	 			<%- end -%>
	 		<%- end -%>
	 	<%- end -%>
    %td{:style => "float: right;padding-right: 30px;"}
      %a.drop{:id => "menu-#{<%= singular_table_name %>.id}", "aria-expanded" => "false", "aria-haspopup" => "true", "data-toggle" => "dropdown"}
        %i.icon-options-vertical
      %ul.actions.dropdown-menu{"aria-labelledby" => "menu-#{<%= singular_table_name %>.id}"}
        - if can? :show, <%= singular_table_name.camelcase %>
          %li
            = link_to admin_<%= singular_table_name %>_path(<%= singular_table_name %>), class: 'new-menu', title: 'Show' do
              %i.icon-eye
              Show
        - if can? :update, <%= singular_table_name.camelcase %>
          %li
            = link_to edit_admin_<%= singular_table_name %>_path(<%= singular_table_name %>), class: 'new-menu', title: 'Edit' do
              %i.icon-pencil
              Edit
        -# - if can? :clone, <%= singular_table_name.camelcase %>
        -#   %li
        -#     = link_to admin_<%= singular_table_name %>_path(<%= singular_table_name %>), class: 'new-menu', title: 'Show' do
        -#       %i.material-icons clone
        -#       Clone
        - if can? :delete, <%= singular_table_name.camelcase %>
          %li
            = link_to admin_<%= singular_table_name %>_path(<%= singular_table_name %>), class: 'new-menu', title: 'Delete', method: :delete, data: { confirm: t("keppler.messages.data_confirm") } do
              %i.icon-trash
              Delete
