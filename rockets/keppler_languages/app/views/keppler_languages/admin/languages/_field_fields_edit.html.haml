.nested-fields.row{ id: "field-#{f.object.id}" }
  .col-lg-4
    = f.input :key, label: 'Nombre'
  .col-lg-4
    = f.input :value, label: 'Valor'
  .col-lg-4
    = link_to admin_languages_languages_path(@language.id, f.object.id.to_i), class: "btn btn-default btn-delete-field" , method: :delete, data: { confirm: t("keppler.messages.data_confirm") } do
      %i.icon-trash
