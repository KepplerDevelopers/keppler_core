- objects.each do |elliot|
  %tr{ id: "row-#{elliot.id}" }
    %td.item-check{ style: 'width: 10%' }
      - if can?(KepplerStarklord::Elliot).destroy_multiple?
        .checkbox
          %label{ for: "checkbox-#{elliot.id}" }
            %input{ type:"checkbox", id: "checkbox-#{elliot.id}", 'checklist-model' => 'check', 'checklist-value' => "#{elliot.id}", '@click' => "listDelete(#{elliot.id})"}
            %span.cr
              %i.cr-icon.glyphicon.glyphicon-ok
    %td
      = link_to admin_starklord_elliot_path(elliot, search: @query), remote: true, class: 'show-row' do
        %span
          - unless elliot.avatar.url.blank?
            = image_tag elliot.avatar.url
    %td.hidden-xs.hidden-sm
      = link_to admin_starklord_elliot_path(elliot, search: @query), remote: true, class: 'show-row' do
        %span
          = elliot.name
    %td{ style: 'width: 15px;padding-right:15px;' }
      %a.action-btn.pull-right{:id => "menu-#{elliot.id}", "aria-expanded" => "false", "aria-haspopup" => "true", "data-toggle" => "dropdown"}
        %i.icon-options-vertical
      %ul.actions.dropdown-menu{"aria-labelledby" => "menu-#{elliot.id}" }
        - if can?(KepplerStarklord::Elliot).index?
          %li
            = link_to admin_starklord_elliot_path(elliot), class: 'new-menu', title: t('keppler.actions.show') do
              %i.icon-eye
              = t('keppler.actions.show')
        - if can?(KepplerStarklord::Elliot).update?
          %li
            = link_to edit_admin_starklord_elliot_path(elliot), class: 'new-menu', title: t('keppler.actions.edit') do
              %i.icon-pencil
              = t('keppler.actions.edit')
        - if can?(KepplerStarklord::Elliot).clone?
          %li
            = link_to admin_starklord_elliot_clone_path(elliot), class: 'new-menu', title: t('keppler.actions.clone') do
              %i.icon-docs
              = t('keppler.actions.clone')
        - if can?(KepplerStarklord::Elliot).destroy?
          %li
            = link_to admin_starklord_elliot_path(elliot), class: 'new-menu', title: t('keppler.actions.delete'), method: :delete, data: { confirm: t("keppler.messages.data_confirm") } do
              %i.icon-trash
              = t('keppler.actions.delete')

    %td{:style => 'width: 5%;'}
      %a.drop{:href=>"#"}
        %i.icon-cursor-move