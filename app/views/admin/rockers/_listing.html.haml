- objects.each do |rocker|
  %tr{ id: "row-#{rocker.id}" }
    %td.item-check{ style: 'width: 10%' }
      - if can? :destroy_multiple, Rocker
        .checkbox
          %label{ for: "checkbox-#{rocker.id}" }
            %input{ type:"checkbox", id: "checkbox-#{rocker.id}", 'checklist-model' => 'check', 'checklist-value' => "#{rocker.id}", '@click' => "listDelete(#{rocker.id})"}
            %span.cr
              %i.cr-icon.glyphicon.glyphicon-ok
    %td
      = link_to admin_rocker_path(rocker, search: @query), remote: true, class: 'show-row' do
        %span
          = image_tag rocker.avatar
    %td.hidden-xs
      = link_to admin_rocker_path(rocker, search: @query), remote: true, class: 'show-row' do
        %span
          = rocker.name
    %td{ style: 'padding-right: 30px' }
      %a.drop.pull-right{:id => "menu-#{rocker.id}", "aria-expanded" => "false", "aria-haspopup" => "true", "data-toggle" => "dropdown"}
        %i.icon-options-vertical
      %ul.actions.dropdown-menu{"aria-labelledby" => "menu-#{rocker.id}" }
        - if can? :show, Rocker
          %li
            = link_to admin_rocker_path(rocker), class: 'new-menu', title: t('keppler.actions.show') do
              %i.icon-eye
              = t('keppler.actions.show')
        - if can? :update, Rocker
          %li
            = link_to edit_admin_rocker_path(rocker), class: 'new-menu', title: t('keppler.actions.edit') do
              %i.icon-pencil
              = t('keppler.actions.edit')
        - if can? :clone, Rocker
          %li
            = link_to admin_rocker_clone_path(rocker), class: 'new-menu', title: t('keppler.actions.clone') do
              %i.icon-docs
              = t('keppler.actions.clone')
        - if can? :destroy, Rocker
          %li
            = link_to admin_rocker_path(rocker), class: 'new-menu', title: t('keppler.actions.delete'), method: :delete, data: { confirm: t("keppler.messages.data_confirm") } do
              %i.icon-trash
              = t('keppler.actions.delete')
