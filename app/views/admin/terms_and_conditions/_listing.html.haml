%tr{ id: "row-#{terms_and_condition.id}" }
	- if can? :destroy_multiple, TermsAndCondition
		%td.center.item-check
			= link_to '', admin_terms_and_condition_path(terms_and_condition), remote: true, class: 'show-row'
			%input{ type:'checkbox', class: 'filled-in', id: "checkbox-#{terms_and_condition.id}", 'checklist-model'=>'check', 'checklist-value'=>"#{terms_and_condition.id}", 'ng-click'=>"selectByCheck(#{@objects.size})"}
			%label{ for: "checkbox-#{terms_and_condition.id}" }

	%td.center.btn-w5
		= link_to admin_terms_and_condition_path(terms_and_condition), class: 'btn-floating waves-effect btn-flat', title: 'Show' do
			= material_icon.md_24.remove_red_eye.css_class('md-dark')
	- if can? :update, TermsAndCondition
		%td.center.btn-w5
			= link_to edit_admin_terms_and_condition_path(terms_and_condition), class: 'btn-floating btn-flat tooltipped', title: 'Edit' do
				= material_icon.md_18.create.css_class('md-dark')
	- if can? :clone, TermsAndCondition
		%td.center.btn-w5
			= link_to admin_terms_and_condition_clone_path(terms_and_condition), class: 'btn-floating waves-effect btn-flat', title: 'Clone' do
				= material_icon.md_24.content_copy.css_class('md-dark')
	- if can? :delete, TermsAndCondition
		%td.center.btn-w5{style:'padding-right: 20px'}
			= link_to admin_terms_and_condition_path(terms_and_condition), class: 'btn-floating btn-flat tooltipped', title: 'Delete', method: :delete, data: { confirm: t('keppler.messages.data_confirm') } do
				= material_icon.md_18.delete.css_class('md-dark')
