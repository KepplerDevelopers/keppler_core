- objects.each do |father|
  %tr{ id: "row-#{father.id}" }
    %td.item-check{ style: 'width: 10%' }
      - if can? :destroy_multiple, Father
        .checkbox
          %label{ for: "checkbox-#{father.id}" }
            %input{ type:"checkbox", id: "checkbox-#{father.id}", 'checklist-model' => 'check', 'checklist-value' => "#{father.id}", 'ng-click' => "selectByCheck(#{@objects.size})", '@click' => "listDelete(#{father.id})"}
            %span.cr
              %i.cr-icon.glyphicon.glyphicon-ok
    %td
      = link_to admin_father_path(father, search: @query), remote: true, class: 'show-row' do
        %span
          = image_tag father.avatar
    %td
      = link_to admin_father_path(father, search: @query), remote: true, class: 'show-row' do
        %span
          = father.name
    %td.visible-lg.visible-md
      = link_to admin_father_path(father, search: @query), remote: true, class: 'show-row' do
        %span
          = father.email
    %td{ style: 'padding-right: 30px' }
      %a.drop.pull-right{:id => "menu-#{father.id}", "aria-expanded" => "false", "aria-haspopup" => "true", "data-toggle" => "dropdown"}
        %i.icon-options-vertical
      %ul.actions.dropdown-menu{"aria-labelledby" => "menu-#{father.id}" }
        - if can? :show, Father
          %li
            = link_to admin_father_path(father), class: 'new-menu', title: t('keppler.actions.show') do
              %i.icon-eye
              = t('keppler.actions.show')
        - if can? :update, Father
          %li
            = link_to edit_admin_father_path(father), class: 'new-menu', title: t('keppler.actions.edit') do
              %i.icon-pencil
              = t('keppler.actions.edit')
        - if can? :clone, Father
          %li
            = link_to admin_father_clone_path(father), class: 'new-menu', title: t('keppler.actions.clone') do
              %i.icon-docs
              = t('keppler.actions.clone')
        - if can? :destroy, Father
          %li
            = link_to admin_father_path(father), class: 'new-menu', title: t('keppler.actions.delete'), method: :delete, data: { confirm: t("keppler.messages.data_confirm") } do
              %i.icon-trash
              = t('keppler.actions.delete')
