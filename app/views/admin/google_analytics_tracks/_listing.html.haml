%tr{ id: "row-#{script.id}" }
	- if can? :destroy_multiple, Script
		%td.item-check{style:"width: 10%"}
			= link_to "", admin_script_path(script), remote: true, class: "show-row"
			%input{ type:"checkbox", class: "filled-in", id: "checkbox-#{script.id}", 'checklist-model'=>"check", 'checklist-value'=>"#{script.id}", "ng-click"=>"selectByCheck(#{@objects.size})"}
			%label{ for: "checkbox-#{script.id}" }

	%td{style:"width: 20%"}
		= link_to "", admin_script_path(script, search: @query), remote: true, class: "show-row"
		%span
			= script.name
	%td{style:"width: 20%"}
		= link_to "", admin_script_path(script, search: @query), remote: true, class: "show-row"
		%span
			= script.tracking_id
	%td{style:"width: 55%"}
		= link_to "", admin_script_path(script, search: @query), remote: true, class: "show-row"
		%span
			= script.url.truncate(100)

	%td{style:"width: 5%"}
		= link_to admin_script_path(script), class: "btn-floating waves-effect btn-flat" do
			-# -# = material_icon.md_24.more_horiz.css_class("md-dark")