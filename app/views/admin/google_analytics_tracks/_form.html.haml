.wrapper
	.wrapper-body.container
		.actions
			= link_to admin_google_analytics_tracks_path, class: "btn-floating btn-flat tooltipped", "data-position" => "bottom", "data-tooltip" => t("keppler.actions.back") do
				= material_icon.md_18.arrow_back.css_class("md-dark")
		.wrapper-inputs
			.card
				.card-content
					= simple_form_for [:admin, @google_analytics_track] do |f|
						.row
							= f.input :name
							= f.input :tracking_id
							= f.input :url

							.form-actions.right
								= f.button :submit, t("keppler.actions.save"), name: "_save"
								= f.button :submit, t("keppler.actions.save_and_add_another"), name: "_add_other"
