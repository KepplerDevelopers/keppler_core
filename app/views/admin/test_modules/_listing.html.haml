- objects.each do |test_module|
  %tr{ id: "row-#{test_module.id}" }
    %td.item-check{ style: 'width: 10%' }
      - if can?(TestModule).destroy_multiple?
        .checkbox
          %label{ for: "checkbox-#{test_module.id}" }
            %input{ type:"checkbox", id: "checkbox-#{test_module.id}", 'checklist-model' => 'check', 'checklist-value' => "#{test_module.id}", '@click' => "listDelete(#{test_module.id})"}
            %span.cr
              %i.cr-icon.glyphicon.glyphicon-ok
    %td
      = link_to admin_test_module_path(test_module, search: @query), remote: true, class: 'show-row' do
        %span
          = image_tag test_module.image.url
    %td.hidden-xs
      = link_to admin_test_module_path(test_module, search: @query), remote: true, class: 'show-row' do
        %span
          = test_module.name
    %td{ style: 'width: 15px;padding-right:15px;' }
      %a.action-btn.pull-right{:id => "menu-#{test_module.id}", "aria-expanded" => "false", "aria-haspopup" => "true", "data-toggle" => "dropdown"}
        %i.icon-options-vertical
      %ul.actions.dropdown-menu{"aria-labelledby" => "menu-#{test_module.id}" }
        - if can?(TestModule).show?
          %li
            = link_to admin_test_module_path(test_module), class: 'new-menu', title: t('keppler.actions.show') do
              %i.icon-eye
              = t('keppler.actions.show')
        - if can?(TestModule).update?
          %li
            = link_to edit_admin_test_module_path(test_module), class: 'new-menu', title: t('keppler.actions.edit') do
              %i.icon-pencil
              = t('keppler.actions.edit')
        - if can?(TestModule).clone?
          %li
            = link_to admin_test_module_clone_path(test_module), class: 'new-menu', title: t('keppler.actions.clone') do
              %i.icon-docs
              = t('keppler.actions.clone')
        - if can?(TestModule).destroy?
          %li
            = link_to admin_test_module_path(test_module), class: 'new-menu', title: t('keppler.actions.delete'), method: :delete, data: { confirm: t("keppler.messages.data_confirm") } do
              %i.icon-trash
              = t('keppler.actions.delete')

    %td{:style => 'width: 5%;'}
      %a.drop{:href=>"#"}
        %i.icon-cursor-move
