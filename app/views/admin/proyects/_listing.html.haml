%tr{ id: "row-#{proyect.id}" }
	- if can? :destroy_multiple, Proyect
		%td.center.item-check
			= link_to '', admin_proyect_path(proyect), remote: true, class: 'show-row'
			%input{ type:'checkbox', class: 'filled-in', id: "checkbox-#{proyect.id}", 'checklist-model'=>'check', 'checklist-value'=>"#{proyect.id}", 'ng-click'=>"selectByCheck(#{@objects.size})"}
			%label{ for: "checkbox-#{proyect.id}" }

	%td.center
		= link_to '', admin_proyect_path(proyect, search: @query), remote: true, class: 'show-row'
		%span
			= image_tag proyect.banner, style: 'height: 50px'
	%td.center
		= link_to '', admin_proyect_path(proyect, search: @query), remote: true, class: 'show-row'
		%span
			= proyect.headline
	%td.center
		= link_to '', admin_proyect_path(proyect, search: @query), remote: true, class: 'show-row'
		%span
			= proyect.service_type
	%td.center
		= link_to '', admin_proyect_path(proyect, search: @query), remote: true, class: 'show-row'
		%span
			= proyect.name
	%td.center
		= link_to '', admin_proyect_path(proyect, search: @query), remote: true, class: 'show-row'
		%span
			= proyect.share
	%td.center.btn-w5
		= link_to admin_proyect_path(proyect), class: 'btn-floating waves-effect btn-flat', title: 'Show' do
			= material_icon.md_24.remove_red_eye.css_class('md-dark')
	- if can? :update, Proyect
		%td.center.btn-w5
			= link_to edit_admin_proyect_path(proyect), class: 'btn-floating btn-flat tooltipped', title: 'Edit' do
				= material_icon.md_18.create.css_class('md-dark')
	- if can? :clone, Proyect
		%td.center.btn-w5
			= link_to admin_proyect_clone_path(proyect), class: 'btn-floating waves-effect btn-flat', title: 'Clone' do
				= material_icon.md_24.content_copy.css_class('md-dark')
	- if can? :delete, Proyect
		%td.center.btn-w5{style:'padding-right: 20px'}
			= link_to admin_proyect_path(proyect), class: 'btn-floating btn-flat tooltipped', title: 'Delete', method: :delete, data: { confirm: t('keppler.messages.data_confirm') } do
				= material_icon.md_18.delete.css_class('md-dark')
